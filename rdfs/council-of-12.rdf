<?xml version="1.0"?>

<rdf:RDF xml:lang="en" 
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" 
  xmlns:foaf="http://xmlns.com/foaf/0.1/" 
  xmlns:xsd="http://www.w3.org/2001/XMLSchema#" 
  xmlns:cggroot="http://www.cgg.sm/ontology/cgg#" 
  xmlns:cggbase="http://www.cgg.sm/ontology/cgg#" 
  xmlns:eli="http://data.europa.eu/eli/ontology#" 
  xmlns:vc="http://www.w3.org/2006/vcard/ns#">

    <!-- COUNCIL OF 12 -->

    <rdfs:Class rdf:ID="CouncilOfTwelves">
      <rdfs:label xml:lang="en">Council of Twelves</rdfs:label>
      <rdfs:label xml:lang="it">Consiglio dei XII</rdfs:label>
      <rdfs:subClassOf rdf:resource="http://www.cgg.sm/ontology/base#Institution"/>
    </rdfs:Class>

    <rdfs:Property rdf:ID="composedByCouncilors">      <!-- subclass of councilor dedicated to the commission? -->
      <rdfs:subPropertyOf rdf:resource="http://www.cgg.sm/ontology/base#composedBy"/>
      <rdfs:domain rdf:resource="#CouncilCommission"/>
      <rdfs:range rdf:resource="http://www.cgg.sm/ontology/cgg#Councilor"/>
    </rdfs:Property>

    <rdfs:Property rdf:ID="functionsOfCouncilOfTwelves">
      <rdfs:domain rdf:resource="#CouncilOfTwelves"/>
      <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    </rdfs:Property>

</rdf:RDF>