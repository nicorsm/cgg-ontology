<?xml version="1.0"?>

<rdf:RDF xml:lang="en" 
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" 
  xmlns:foaf="http://xmlns.com/foaf/0.1/"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  >


  <!-- BASE -->
  <rdfs:Class rdf:ID="Institution" />
  <rdf:Property rdf:ID="belongsTo" />
  <rdfs:Property rdf:ID="composedBy" />
  <rdfs:Property rdf:ID="functions" />
  <rdfs:Class rdf:ID="Politician"> <!-- UK ontology? -->
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/#term_Person"/>
  </rdfs:Class>

  <!-- GRAND AND GENERAL COUNCIL -->

  <rdfs:Class rdf:ID="GrandAndGeneralCouncil">
    <rdfs:label xml:lang="en">Grand and General Council</rdfs:label>
    <rdfs:label xml:lang="it">Consiglio Grande e Generale</rdfs:label>
    <rdfs:comment>Institution responsible for the legislative power, the determination of political address and the exercise of control functions. Made up of 60 members called Councilors.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="#Institution"/>
  </rdfs:Class>

  <rdfs:Class rdf:ID="Councilor">
    <rdfs:label xml:lang="en">Councilor</rdfs:label>
    <rdfs:label xml:lang="it">Consigliere</rdfs:label>
    <rdfs:subClassOf rdf:resource="#Politician"/>
    <rdfs:comment>One of the 60 members of the GGC, belonging to a political force or mixed group.</rdfs:comment>
  </rdfs:Class>

  <rdf:Property rdf:ID="declares">
    <rdfs:domain rdf:resource="#Councilor"/>
    <rdfs:range rdf:resource="#TaxReturn"/>
  </rdf:Property>

  <rdfs:Class rdf:ID="TaxReturn">
    <rdfs:domain rdf:resource="#Councilor"/>
    <rdfs:label xml:lang="en">Tax Return</rdfs:label>
    <rdfs:label xml:lang="it">Dichiarazione dei Redditi</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:comment>The tax return declarated by each Councilor.</rdfs:comment>
  </rdfs:Class>

  <rdf:Property rdf:ID="belongsToGroup">
    <rdfs:subPropertyOf rdf:resource="#belongsTo"/>
    <rdfs:domain rdf:resource="#Councilor"/>
    <rdfs:range rdf:resource="#ParliamentaryGroup"/>
  </rdf:Property>

  <rdfs:Class rdf:ID="ParliamentaryGroup">
    <rdfs:label xml:lang="en">Parliamentary Group</rdfs:label>
    <rdfs:label xml:lang="it">Gruppo parlamentare</rdfs:label>
  </rdfs:Class>

  <rdfs:Property rdf:ID="belongsToFaction">
    <rdfs:subPropertyOf rdf:resource="#belongsTo"/>
    <rdfs:domain rdf:resource="#ParliamentaryGroup"/>
    <rdfs:range rdf:resource="#Majority"/>
    <rdfs:range rdf:resource="#Minority"/>
  </rdfs:Property>

  <rdfs:Class rdf:ID="CounciliarGroup">
    <rdfs:label xml:lang="en">Counciliar Group</rdfs:label>
    <rdfs:label xml:lang="it">Gruppo Consiliare</rdfs:label>
    <rdfs:subClassOf rdf:resource="#ParliamentaryGroup"/>
  </rdfs:Class>

  <rdfs:Class rdf:ID="Majority">
    <rdfs:label xml:lang="en">Majority</rdfs:label>
    <rdfs:label xml:lang="it">Maggioranza</rdfs:label>
  </rdfs:Class>

  <rdfs:Class rdf:ID="Minority">
    <rdfs:label xml:lang="en">Minority</rdfs:label>
    <rdfs:label xml:lang="it">Minoranza</rdfs:label>
  </rdfs:Class>

  <rdfs:Class rdf:ID="Coalition">
    <rdfs:label xml:lang="en">Coalition</rdfs:label>
    <rdfs:label xml:lang="it">Coalizione</rdfs:label>
  </rdfs:Class>

  <rdfs:Property rdf:ID="coalitionComposedBy">
    <rdfs:domain rdf:resource="#Coalition"/>
    <rdfs:range rdf:resource="#CounciliarGroup"/>
    <rdfs:subPropertyOf rdf:resource="#composedBy"/>
  </rdfs:Property>

  <!--<rdfs:Property rdf:ID="hasLeader">
    <rdfs:domain rdf:resource="#CounciliarGroup" />
    <rdfs:range rdf:resource="#Leader" />
  </rdfs:Property>-->

  <rdfs:Property rdf:ID="leader">
    <rdfs:label xml:lang="en">Leader</rdfs:label>
    <rdfs:label xml:lang="it">Capogruppo</rdfs:label>
    <!--<rdfs:subClassOf rdf:resource="#Councilor" />-->
    <rdfs:domain rdf:resource="#CounciliarGroup"/>
    <rdfs:range rdf:resource="#Councilor"/>
  </rdfs:Property>

  <rdfs:Class rdf:ID="MixedGroup">
    <rdfs:label xml:lang="en">Mixed Group</rdfs:label>
    <rdfs:label xml:lang="it">Gruppo Misto</rdfs:label>
    <rdfs:subClassOf rdf:resource="#ParliamentaryGroup"/>
  </rdfs:Class>

  <rdfs:Property rdf:ID="electedFor">
    <rdfs:domain rdf:resource="#GrandAndGeneralCouncil"/>
    <rdfs:range rdf:resource="#LegislativePeriod"/>
  </rdfs:Property>

  <rdfs:Class rdf:ID="LegislativePeriod">
    <rdfs:label xml:lang="en">Legislative Period</rdfs:label>
    <rdfs:label xml:lang="it">Legislatura</rdfs:label>
  </rdfs:Class>

  <rdfs:Property rdf:ID="composedByGroups">
    <rdfs:domain rdf:resource="#GrandAndGeneralCouncil"/>
    <rdfs:range rdf:resource="#CounciliarGroup"/> <!--union of all ranges: doesn't work!! -->
    <rdfs:range rdf:resource="#MixedGroup"/>  <!--union of all ranges: doesn't work!! -->
    <rdfs:subPropertyOf rdf:resource="#composedBy"/>
  </rdfs:Property>

  <!-- REGENCY -->

  <rdfs:Property rdf:ID="elects">
    <rdfs:domain rdf:resource="#GrandAndGeneralCouncil"/>
    <rdfs:range rdf:resource="#Regency"/>
  </rdfs:Property>

  <rdfs:Class rdf:ID="Regency">
    <rdfs:label xml:lang="en">Regency</rdfs:label>
    <rdfs:label xml:lang="it">Reggenza</rdfs:label>
    <rdfs:subClassOf rdf:resource="#Institution"/>
  </rdfs:Class>

  <rdfs:Property rdf:ID="startDate">
    <rdfs:domain rdf:resource="#Regency"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </rdfs:Property>

  <rdfs:Property rdf:ID="endDate">
    <rdfs:domain rdf:resource="#Regency"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
  </rdfs:Property>

  <rdfs:Property rdf:ID="composedByRegents">
    <rdfs:domain rdf:resource="#Regency"/>
    <rdfs:range rdf:resource="#CaptainRegent"/>
    <rdfs:subPropertyOf rdf:resource="#composedBy"/>
  </rdfs:Property>

  <rdfs:Class rdf:ID="CaptainRegent">
    <rdfs:label xml:lang="en">Captain Regent</rdfs:label>
    <rdfs:label xml:lang="it">Capitano Reggente</rdfs:label>
    <rdfs:subClassOf rdf:resource="#Councilor"/>
  </rdfs:Class>

  <!-- CONGRESS -->

  <rdfs:Class rdf:ID="Congress">
    <rdfs:label xml:lang="en">Congress of State</rdfs:label>
    <rdfs:label xml:lang="it">Congresso di Stato</rdfs:label>
    <rdfs:subClassOf rdf:resource="#Institution"/>
  </rdfs:Class>

  <rdfs:Property rdf:ID="composedByMinisters">
    <rdfs:domain rdf:resource="#Congress"/>
    <rdfs:range rdf:resource="#Minister"/>
    <rdfs:subPropertyOf rdf:resource="#composedBy"/>
  </rdfs:Property>

  <rdfs:Class rdf:ID="Minister">
    <rdfs:label xml:lang="en">Minister</rdfs:label>
    <rdfs:label xml:lang="it">Segretario di Stato</rdfs:label>
    <rdfs:subClassOf rdf:resource="#Politician"/>
  </rdfs:Class>

  <rdfs:Property rdf:ID="holds">
    <rdfs:domain rdf:resource="#Minister"/>
    <rdfs:range rdf:resource="#Secretary"/>
  </rdfs:Property>

  <rdfs:Class rdf:ID="Secretary">
    <rdfs:label xml:lang="en">Secretary of State</rdfs:label>
    <rdfs:label xml:lang="it">Segreteria di Stato</rdfs:label>
    <!-- Address / Vcard? -->
    <rdfs:subClassOf rdf:resource="#Institution"/>
  </rdfs:Class>

  <rdfs:Property rdf:ID="entrustedOf">
    <rdfs:domain rdf:resource="#Secretary"/>
    <rdfs:range rdf:resource="#Delegation"/>
  </rdfs:Property>

  <rdfs:Class rdf:ID="Delegation">  <!-- Department? -->
    <rdfs:label xml:lang="en">Delegation</rdfs:label>
    <rdfs:label xml:lang="it">Delega</rdfs:label>
  </rdfs:Class>

  <!-- COMMISSIONS -->

  <rdfs:Class rdf:ID="CouncilCommission">
    <rdfs:label xml:lang="en">Council Commission</rdfs:label>
    <rdfs:label xml:lang="it">Commissione consiliare</rdfs:label>
  </rdfs:Class>

  <rdfs:Class rdf:ID="PermanentCouncilCommission">
    <rdfs:subClassOf rdf:resource="#CouncilCommission"/>
  </rdfs:Class>

  <rdfs:Class rdf:ID="SpecialCouncilCommission">
    <rdfs:subClassOf rdf:resource="#CouncilCommission"/>
  </rdfs:Class>

  <rdfs:Property rdf:ID="composedByCouncilors"> <!-- subclass of councilor dedicated to the commission? -->
    <rdfs:subPropertyOf rdf:resource="#composedBy"/>
    <rdfs:domain rdf:resource="#CouncilCommission"/>
    <rdfs:range rdf:resource="#Councilor"/>
  </rdfs:Property>

  <rdfs:Property rdf:ID="hasPresident"> <!-- is it correct? double properties -->
    <rdfs:domain rdf:resource="#CouncilCommission"/>
    <rdfs:range rdf:resource="#Councilor"/>
  </rdfs:Property>

  <rdfs:Property rdf:ID="hasVicePresident">
    <rdfs:domain rdf:resource="#CouncilCommission"/>
    <rdfs:range rdf:resource="#Councilor"/>
  </rdfs:Property>

  <rdfs:Property rdf:ID="inChargeFor">
    <rdfs:domain rdf:resource="#CouncilCommission"/>
    <rdfs:range rdf:resource="#LegislativePeriod"/>
  </rdfs:Property>

  <!-- COUNCIL OF 12 -->

  <rdfs:Class rdf:ID="CouncilOfTwelves">
    <rdfs:label xml:lang="en">Council of Twelves</rdfs:label>
    <rdfs:label xml:lang="it">Consiglio dei XII</rdfs:label>
  </rdfs:Class>

  <rdfs:Property rdf:ID="composedByCouncilors"> <!-- duplicate -->
    <rdfs:subPropertyOf rdf:resource="#composedBy"/>
    <rdfs:domain rdf:resource="#CouncilOfTwelves"/>
    <rdfs:range rdf:resource="#Councilor"/>
  </rdfs:Property>

  <rdfs:Property rdf:ID="functionsOfCouncilOfTwelves">
    <rdfs:domain rdf:resource="#CouncilOfTwelves"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
  </rdfs:Property>

  <!-- INTERNATIONAL DELEGATIONS -->

  <rdfs:Class rdf:ID="CounciliarDelegationAtInternationalOrganizations">
    <rdfs:label xml:lang="en">Counciliar Delegation at International Organization</rdfs:label>
    <rdfs:label xml:lang="it">Delegazione Consiliare presso Organismo Internazionale</rdfs:label>
  </rdfs:Class>

  <rdfs:Property rdf:ID="organizationName">
    <rdfs:domain rdf:resource="#CounciliarDelegationAtInternationalOrganizations"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
  </rdfs:Property>




</rdf:RDF>